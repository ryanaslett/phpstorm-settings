<filetype binary="false" description="Awk and Gawk Script types" name="Awk/Gawk">
  <highlighting>
    <options>
      <option name="LINE_COMMENT" value="#" />
      <option name="COMMENT_START" value="" />
      <option name="COMMENT_END" value="" />
      <option name="HEX_PREFIX" value="" />
      <option name="NUM_POSTFIXES" value="" />
      <option name="HAS_BRACES" value="true" />
      <option name="HAS_BRACKETS" value="true" />
      <option name="HAS_PARENS" value="true" />
      <option name="HAS_STRING_ESCAPES" value="true" />
    </options>
    <keywords keywords="gsub;length;match;split;substr" ignore_case="false" />
    <keywords3 keywords="sprintf;strftime" />
    <keywords4 keywords="BEGIN;END;print" />
  </highlighting>
  <extensionMap>
    <mapping pattern="scratch_12.awk" />
    <mapping ext="gawk" />
    <mapping ext="awk" />
  </extensionMap>
</filetype>